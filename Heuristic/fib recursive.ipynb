    "def rec_fib(number):\n",
    "    #counter = 0\n",
    "    if number < 0:\n",
    "        return(\"incorrect input\")\n",
    "    elif number < 2:\n",
    "        return number\n",
    "    else:\n",
    "        return rec_fib(number-1) + rec_fib(number-2)\n",
    "print(rec_fib(40))"
   ]
  }
